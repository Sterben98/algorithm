/*
 * Complete the 'rotLeft' function below.
 *
 * The function is expected to return an INTEGER_ARRAY.
 * The function accepts following parameters:
 *  1. INTEGER_ARRAY a
 *  2. INTEGER d
 */
 
vector<int> rotLeft(vector<int> a, int d) {
 
    vector<int> b=a;
    
    for(unsigned int i=0; i<a.size(); i++){
        b[i]=a[d];
        if((d)==a.size()-1){
            d=-1;
        }
        d++;
    }
    
    return b;
}
